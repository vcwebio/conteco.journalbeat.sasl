#!/usr/bin/env bash

# START CONFIGURATION
DOLLAR='$'
export CONTECO_DC_GLOBAL_STACKS="journalbeat.sasl"
export CONTECO_DC_GLOBAL_STACKNAMES="conteco_dc_journalbeat"
export CONTECO_DC_GLOBAL_BOOTSTACKS=""
export CONTECO_DC_GLOBAL_RUNSTACKS="conteco_dc_journalbeat"
export CONTECO_DC_GLOBAL_VOLUME_LIST="${DOLLAR}{CONTECO_STACKPREFIX}_journalbeat_volume"
export CONTECO_DC_GLOBAL='$CONTECO_DC_GLOBAL_SERVICE_NAME,$CONTECO_DC_GLOBAL_IMAGE_NAME,$CONTECO_DC_JOURNALBEAT_ENVIRONMENT,$CONTECO_DC_JOURNALBEAT_COMMAND,$CONTECO_DC_JOURNALBEAT_PORTS,$CONTECO_DC_JOURNALBEAT_VOLUMES,$CONTECO_DC_JOURNALBEAT_NETWORKS,$CONTECO_DC_GLOBAL_VOLUMES,$CONTECO_DC_GLOBAL_NETWORKS'
# END CONFIGURATION

# START GLOBAL SETTINGS
export CONTECO_DC_GLOBAL_SERVICE_NAME="${CONTECO_TYPE}"
export CONTECO_DC_GLOBAL_IMAGE_NAME="${CONTECO_TYPE}.${CONTECO_NAME}:${CONTECO_TAG}"
export CONTECO_DC_JOURNALBEAT_ENVIRONMENT="\
'ES_JOURNALBEAT_FILEPATHS=${DOLLAR}{CONTECO_DC_JOURNALBEAT_ENVIRONMENT_FILEPATHS}',\
'ES_JOURNALBEAT_SEEK=${DOLLAR}{CONTECO_DC_JOURNALBEAT_ENVIRONMENT_SEEK}',\
'ES_JOURNALBEAT_INCLUDE_MATCHES=${DOLLAR}{CONTECO_DC_JOURNALBEAT_ENVIRONMENT_INCLUDE_MATCHES}',\
'ES_JOURNALBEAT_OUTPUT_CONSOLE=${DOLLAR}{CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_CONSOLE}',\
'ES_JOURNALBEAT_OUTPUT_ELASTICSEARCH=${DOLLAR}{CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_ELASTICSEARCH}',\
'ES_JOURNALBEAT_OUTPUT_LOGSTASH=${DOLLAR}{CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_LOGSTASH}',\
'ES_JOURNALBEAT_OUTPUT_KAFKA=${DOLLAR}{CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_KAFKA}',\
'ES_JOURNALBEAT_OUTPUT_CONFIG_ELASTICSEARCH_HOSTS=${DOLLAR}{CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_CONFIG_ELASTICSEARCH_HOSTS}',\
'ES_JOURNALBEAT_OUTPUT_CONFIG_ELASTICSEARCH_INDEX=${DOLLAR}{CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_CONFIG_ELASTICSEARCH_INDEX}',\
'ES_JOURNALBEAT_OUTPUT_CONFIG_ELASTICSEARCH_SSL=${DOLLAR}{CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_CONFIG_ELASTICSEARCH_SSL}',\
'ES_JOURNALBEAT_OUTPUT_CONFIG_ELASTICSEARCH_USERNAME=${DOLLAR}{CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_CONFIG_ELASTICSEARCH_USERNAME}',\
'ES_JOURNALBEAT_OUTPUT_CONFIG_ELASTICSEARCH_PASSWORD=${DOLLAR}{CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_CONFIG_ELASTICSEARCH_PASSWORD}',\
'ES_JOURNALBEAT_OUTPUT_CONFIG_LOGSTASH_HOSTS=${DOLLAR}{CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_CONFIG_LOGSTASH_HOSTS}',\
'ES_JOURNALBEAT_OUTPUT_CONFIG_LOGSTASH_INDEX=${DOLLAR}{CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_CONFIG_LOGSTASH_INDEX}',\
'ES_JOURNALBEAT_OUTPUT_CONFIG_KIBANA_HOST=${DOLLAR}{CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_CONFIG_KIBANA_HOST}',\
'ES_JOURNALBEAT_OUTPUT_CONFIG_KIBANA_DASHBOARDS=${DOLLAR}{CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_CONFIG_KIBANA_DASHBOARDS}',\
'ES_JOURNALBEAT_OUTPUT_CONFIG_KAFKA_HOSTS=${DOLLAR}{CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_CONFIG_KAFKA_HOSTS}',\
'ES_JOURNALBEAT_OUTPUT_CONFIG_KAFKA_SSL=${DOLLAR}{CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_CONFIG_KAFKA_SSL}',\
'ES_JOURNALBEAT_OUTPUT_CONFIG_KAFKA_USERNAME=${DOLLAR}{CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_CONFIG_KAFKA_USERNAME}',\
'ES_JOURNALBEAT_OUTPUT_CONFIG_KAFKA_PASSWORD=${DOLLAR}{CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_CONFIG_KAFKA_PASSWORD}',\
'ES_JOURNALBEAT_OUTPUT_CONFIG_KAFKA_TOPIC=${DOLLAR}{CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_CONFIG_KAFKA_TOPIC}'"
export CONTECO_DC_JOURNALBEAT_VOLUMES="'${DOLLAR}{CONTECO_STACKPREFIX}_journalbeat_volume:/usr/share/journalbeat/data','/etc:/hostfs/etc:ro'${DOLLAR}{CONTECO_DC_JOURNALBEAT_ENVIRONMENT_FILEPATHS_MAPPING}"
export CONTECO_DC_JOURNALBEAT_NETWORKS="${DOLLAR}{CONTECO_NETWORKLOCAL}_overlay"
export CONTECO_DC_GLOBAL_VOLUMES="${DOLLAR}{CONTECO_STACKPREFIX}_journalbeat_volume: { external: true }"
export CONTECO_DC_GLOBAL_NETWORKS="${DOLLAR}{CONTECO_NETWORKLOCAL}_overlay: { external: true }"
# END GLOBAL SETTINGS

# START SETTINGS journalbeat.base
export CONTECO_DC_JOURNALBEAT_ENVIRONMENT_FILEPATHS="/hostfs/var/log/journal"
export CONTECO_DC_JOURNALBEAT_ENVIRONMENT_FILEPATHS_MAPPING=",'/var/log/journal:/hostfs/var/log/journal:ro'"
export CONTECO_DC_JOURNALBEAT_ENVIRONMENT_SEEK="head"
export CONTECO_DC_JOURNALBEAT_ENVIRONMENT_INCLUDE_MATCHES=
export CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_CONSOLE="false"
export CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_ELASTICSEARCH="false"
export CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_KAFKA="false"
export CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_LOGSTASH="false"
export CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_CONFIG_ELASTICSEARCH_HOSTS="elasticsearch:9200"
export CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_CONFIG_ELASTICSEARCH_INDEX="journalbeat"
export CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_CONFIG_ELASTICSEARCH_SSL="false"
export CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_CONFIG_ELASTICSEARCH_USERNAME="elasticsearch"
export CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_CONFIG_ELASTICSEARCH_PASSWORD="password"
export CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_CONFIG_LOGSTASH_HOSTS="logstash:5044"
export CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_CONFIG_LOGSTASH_INDEX="journalbeat"
export CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_CONFIG_KIBANA_HOST="kibana:5601"
export CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_CONFIG_KIBANA_DASHBOARDS="false"
export CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_CONFIG_KAFKA_HOSTS="kafka:9092"
export CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_CONFIG_KAFKA_SSL="false"
export CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_CONFIG_KAFKA_USERNAME="kafka"
export CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_CONFIG_KAFKA_PASSWORD="password"
export CONTECO_DC_JOURNALBEAT_ENVIRONMENT_OUTPUT_CONFIG_KAFKA_TOPIC="journalbeat"
export CONTECO_DC_JOURNALBEAT_DEPLOY_MODE="replicated"
export CONTECO_DC_JOURNALBEAT_DEPLOY_RESOURCES_LIMITS_MEMORY="128m"
export CONTECO_DC_JOURNALBEAT_DEPLOY_RESOURCES_RESERVATIONS_MEMORY="8m"
# END SETTINGS journalbeat.base

# START TOPLEVEL SETTINGS
# END TOPLEVEL SETTINGS
